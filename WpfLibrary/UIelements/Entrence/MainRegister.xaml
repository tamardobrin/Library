<UserControl x:Class="WpfLibrary.UIelements.MainRegister"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfLibrary.UIelements"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TabControl x:Name="sample">
            <TabItem Header="customer">
                <local:RegisterUC></local:RegisterUC>
            </TabItem>
            <TabItem Header="worker">
                <local:WorkerEnter></local:WorkerEnter>
            </TabItem>
        </TabControl>
    </Grid>
</UserControl>
